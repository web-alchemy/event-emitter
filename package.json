    "chai": "^4.2.0",
    "change-case": "^3.1.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^2.1.0",
    "esm": "^3.2.25",
    "husky": "^1.3.1",
    "mocha": "^6.2.3",
